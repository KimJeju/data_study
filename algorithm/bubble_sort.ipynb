   "source": [
    "def bubble_sort(alist):\n",
    "    for i in range(len(alist) -1):\n",
    "        if alist[i] > alist[i + 1]:\n",
    "            alist[i], alist[i + 1] = alist[i + 1], alist[i]\n",
    "    print(alist)"
   ]
  },
  {

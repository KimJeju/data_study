    "def solution(my_str):\n",
    "    max_length = len(my_str) - 1\n",
    "\n",
    "    for idx in range(max_length + 1):\n",
    "        char_list.append(my_str[max_length - idx])\n",
    "        answer = ''.join(char_list)\n",
    "    return answer"
   ]
